{
  "name": "skylark-d3",
  "description": "A version of d3 that ported to running on skylarkjs",
  "version": "0.9.2",
  "keywords": [
    "skylark",
    "chart.js"
  ],
  "homepage": "https://github.com/skylark-integration/skylark-d3/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "sbundle build ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skylark-integration/skylark-d3.git"
  },
  "bugs": {
    "url": "https://github.com/skylark-integration/skylark-d3/issues"
  },
  "license": "MIT",
  "dependencies": {
    "skylark-langx": "git+https://github.com/skylarklangx/skylark-langx.git#master"
  },
  "skylarkjs": {
    "build": {
      "prepare": {
        "texttojs": [
          "css",
          "html"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      }
    }
  },
  "engines": {
    "node": ">=0.10.1"
  }
}
